{
	"manifest_version": 2,
	"name": "Auto Scroll",
	"version": "0.1.0",
	"description": "auto scroll page",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "auto scroll"
	},

	"permissions": [
		"tabs"
	],

	"content_scripts": [
		{
			"matches": ["https://developer.chrome.com/*"],
			"js": ["content.js"]
		}
	]
}